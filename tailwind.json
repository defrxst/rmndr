{
	"m-auto": {
		"style": {
			"marginTop": "auto",
			"marginRight": "auto",
			"marginBottom": "auto",
			"marginLeft": "auto"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"w-11/12": {
		"style": {
			"width": "91.666667%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-xl": {
		"style": {
			"borderTopLeftRadius": 12,
			"borderTopRightRadius": 12,
			"borderBottomRightRadius": 12,
			"borderBottomLeftRadius": 12
		}
	},
	"border-2": {
		"style": {
			"borderTopWidth": 2,
			"borderRightWidth": 2,
			"borderBottomWidth": 2,
			"borderLeftWidth": 2
		}
	},
	"border-black": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(0 0 0 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(0 0 0 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(0 0 0 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(0 0 0 / var(--tw-border-opacity))"
		}
	},
	"p-2": {
		"style": {
			"paddingTop": 8,
			"paddingRight": 8,
			"paddingBottom": 8,
			"paddingLeft": 8
		}
	},
	"p-5": {
		"style": {
			"paddingTop": 20,
			"paddingRight": 20,
			"paddingBottom": 20,
			"paddingLeft": 20
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-6xl": {
		"style": {
			"fontSize": 60,
			"lineHeight": 1
		}
	},
	"text-3xl": {
		"style": {
			"fontSize": 30,
			"lineHeight": 36
		}
	},
	"text-4xl": {
		"style": {
			"fontSize": 36,
			"lineHeight": 40
		}
	},
	"text-5xl": {
		"style": {
			"fontSize": 48,
			"lineHeight": 1
		}
	},
	"font-extrabold": {
		"style": {
			"fontWeight": "800"
		}
	},
	"text-black": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(0 0 0 / var(--tw-text-opacity))"
		}
	}
}